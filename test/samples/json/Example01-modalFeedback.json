"responseProcessing":{"serial":"response_custom_530200e5715ca750457500",
"qtiClass":"responseProcessing",
"attributes":{},
"processingType":"custom",
"data":"<responseProcessing>\n        <responseCondition>\n            <responseIf>\n                <match>\n\n                    <!--\ufffc\ufffc\ufffc\ufffcThe value of RESPONSE is compared with the correct value identified \n                        in the RESPONSE declaration-->\n\n                    <variable identifier=\"RESPONSE\"\/>\n                    <correct identifier=\"RESPONSE\"\/>\n                <\/match>\n                <setOutcomeValue identifier=\"SCORE\">\n                    <variable identifier=\"MAXSCORE\"\/>\n                <\/setOutcomeValue>\n                <setOutcomeValue identifier=\"FEEDBACK\">\n                    <baseValue baseType=\"identifier\">correct<\/baseValue>\n                <\/setOutcomeValue>\n            <\/responseIf>\n            <responseElse>\n\n                <!--\ufffcDepending on whether the input matches the correct answer or not, FEEDBACK \n                    is given the value of the identifier of the appropriate feedback message-->\n\n                <setOutcomeValue identifier=\"FEEDBACK\">\n                    <baseValue baseType=\"identifier\">incorrect<\/baseValue>\n                <\/setOutcomeValue>\n            <\/responseElse>\n        <\/responseCondition>\n    <\/responseProcessing>"
}
},
