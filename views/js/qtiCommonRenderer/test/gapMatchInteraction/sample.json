    "body": {
        "serial": "container_containeritembody_57234defdc6a5232644642",
        "body": "\n        <div class=\"grid-row\">\n            <div class=\"col-12\">\n                <h2>Do you know the french poet Charles Baudelaire ?<\/h2>\n            <\/div>\n        <\/div>\n        <div class=\"grid-row\">\n            {{interaction_gapmatchinteraction_57234defdde98169355437}}\n        <\/div>\n    ",
        "elements": {
            "interaction_gapmatchinteraction_57234defdde98169355437": {
